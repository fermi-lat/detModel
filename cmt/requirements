package detModel
# $Header:$
version v3

use xmlUtil 

include_dirs "${DETMODELROOT}"

apply_pattern package_linkopts

private

macro headers "" WIN32\
     ../detModel/Management/*.h \
     ../detModel/Sections/*.h \
     ../detModel/Constants/*.h \
     ../detModel/Utilities/*.h 

library detModel -no_share $(headers) \
     Management/*.cpp \
     Sections/*.cpp \
     Constants/*.cpp \
     Utilities/*.cpp 
	
    


application test main.cxx 




